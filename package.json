{
  "name": "github.testing",
  "version": "1.0.0",
  "description": "Githib workflows POC",
  "author": "PHilA1",
  "private": true,
  "release": {
      "branches": ["main"]
  },
  "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github", 
      {
          "successComment": false,
          "failComment": false
      }
  ]
}